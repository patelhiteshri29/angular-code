{
  "name": "apex-ui",
  "version": "1.92.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "",
    "postinstall": "npm run ngcompiler --quiet",
    "ng": "ng",
    "test": "node --max_old_space_size=7168 ./node_modules/@angular/cli/bin/ng test",
    "ngcompiler": "ngcc --properties es2015 browser module main --loglevel error",
    "start": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng serve --project=apex-ui --proxy-config proxy.conf.ts --host 0.0.0.0 --disable-host-check true --ssl true --ssl-key ./docker/certs/cert.key --ssl-cert ./docker/certs/cert.pem --port 443",
    "build": "node --max_old_space_size=3584 ./node_modules/@angular/cli/bin/ng build --project=apex-ui --namedChunks=false && npm test",
    "build-prod": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --project=apex-ui --configuration production --source-map=false --no-progress && npm test",
    "stats": "npm run build-prod -- --source-map=true --stats-json && npx webpack-bundle-analyzer dist/apps/apex-ui/stats.json",
    "lint:all": "npx eslint --quiet --fix --ignore-path .prettierignore \"{apps,libs,packages}/**/*.{js,ts,html}\"",
    "lint:apps": "npx eslint --quiet --fix --ignore-path .prettierignore \"apps/**/**/*.{js,ts,html}\"",
    "lint:libs": "npx eslint --quiet --fix --ignore-path .prettierignore \"libs/**/*.{js,ts,html}\"",
    "lint:packages": "npx eslint --quiet --fix --ignore-path .prettierignore \"packages/**/*.{js,ts,html}\"",
    "prettier:format": "npx prettier \"{apps,libs,packages}/**/*.{js,ts,html}\" --write",
    "prettier:verify": "npx prettier --check  \"{apps,libs,packages}/**/*.{js,ts,html}\"",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.app.json",
    "compress": "npx gzipper compress --verbose ./dist --exclude=png,jpg,gif,woff,pdf"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.fis.dev/scm/fpapex/apex-ui.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.fis.dev/artifactory/api/npm/fpapex-npm-release/"
  },
  "files": [
    "dist/apps/apex-ui"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "{apps,libs,packages}/**/*.{js,ts,html}": [
      "eslint --quiet --fix --ignore-path .prettierignore",
      "prettier --write"
    ]
  },
  "resolutions": {
    "uglify-es": "npm:terser",
    "d3-scale": "2.2.2",
    "d3-array": "1.2.4",
    "d3-time": "1.1.0"
